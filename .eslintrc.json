{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "react-hooks/exhaustive-deps": "off",  // Disables the missing dependency warning in useEffect
    "@typescript-eslint/no-unused-vars": "off",  // Disables the warning for unused variables
    "@typescript-eslint/no-empty-interface": "off",  // Disables the warning for empty interfaces
    "@typescript-eslint/no-empty-object-type": "off"  // Disables the warning for empty object types
  }
}
